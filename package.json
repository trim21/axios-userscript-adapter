{
  "name": "axios-userscript-adapter",
  "version": "0.2.0",
  "description": "GM.xmlHttpRequest adapter for Axios HTTP client",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "prebuild": "rimraf ./dist/",
    "build": "tsc && tsc -p tsconfig.cjs.json && rollup -c rollup.config.js",
    "postbuild": "rimraf ./dist/dist/",
    "version": "npm run build",
    "lint": "eslint ./**/*.ts",
    "prettier": "prettier ./*",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Trim21/axios-userscript-adapter.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "axios",
    "adapter",
    "userscript",
    "greasemonkey",
    "tampermonkey"
  ],
  "author": {
    "name": "Trim21",
    "email": "trim21me@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Trim21/axios-userscript-adapter/issues"
  },
  "homepage": "https://github.com/Trim21/axios-userscript-adapter",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1 || ^13.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/greasemonkey": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "axios": "^1.0.0-alpha.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "prettier": "^2.7.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "lint-staged": {
    "*.{ts,js,json,yaml,yml}": "prettier -w"
  },
  "peerDependencies": {
    "axios": "^1.0.0-alpha.1"
  },
  "dependencies": {
    "lint-staged": "^12.5.0 || ^13.0.0"
  }
}
